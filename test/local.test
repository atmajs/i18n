
include
	.js('/lib/localization.js?path=/test/localization/%%.json')
	.done(function(){
		
		var req = {
			getHeader: function(){
				return 'en-US,en;q=0.8,ru;q=0.6,de;q=0.4';
			}
		};
		
		UTest({
			
			simple: function(){
				
				eq($L.fromReq(req)('foo'), 'Bar');
				eq($L.fromReq(req)('foo-num', 10), 'Bar 10!');
			}
		});
	});